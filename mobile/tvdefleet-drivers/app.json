{
  "expo": {
    "name": "TVDEFleet Drivers",
    "slug": "tvdefleet-drivers",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "owner": "paulodiramos",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e40af"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tvdefleet.drivers",
      "buildNumber": "6",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "A aplicação precisa de acesso à localização para registar o ponto de trabalho.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "A aplicação precisa de acesso à localização para registar o ponto de trabalho.",
        "UIBackgroundModes": ["location"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1e40af"
      },
      "package": "com.tvdefleet.drivers",
      "versionCode": 6,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "FOREGROUND_SERVICE",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1d78dad4-104f-4f69-b8e5-82640aad8635"
      },
      "apiUrl": "https://tvdefleet.com/api"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/1d78dad4-104f-4f69-b8e5-82640aad8635"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24,
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          },
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "A aplicação precisa de acesso à localização para registar o ponto de trabalho.",
          "isAndroidBackgroundLocationEnabled": true
        }
      ]
    ]
  }
}
